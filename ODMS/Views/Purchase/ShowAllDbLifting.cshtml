@model IEnumerable<ODMS.Models.RPT_Realtime_DBLifting_Result>



<div class="panel panel-default">
    <div class="panel-heading">
        <div class="clearfix ">

        </div>
    </div>
    <div class="panel-body">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.REGION_Name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AREA_Name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.CEAREA_Name)
                    </th>

                    <th>
                        @Html.DisplayNameFor(model => model.DB_Name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.DBCode)
                    </th>

                    <th>
                        @Html.DisplayNameFor(model => model.ChallanNo)
                    </th>

                    <th>
                        @Html.DisplayNameFor(model => model.ChallanDate)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ReceivedDate)
                    </th>

                    <th>
                        @Html.DisplayNameFor(model => model.ChallanQty)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ReciveQty)
                    </th>
                    <th>Actions</th>
                </tr>
            </thead>

            <tbody>


                @foreach (var item in Model)
                {
                    <tr>





                        <td>
                            @Html.DisplayFor(modelItem => item.REGION_Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AREA_Name)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.CEAREA_Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DB_Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DBCode)
                        </td>
                     

                        <td>
                            @Html.DisplayFor(modelItem => item.ChallanNo)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.ChallanDate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ReceivedDate)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.ChallanQty)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ReciveQty)
                        </td>
                        <td>
                            @Html.ActionLink("Details", "LiftingDetails", new { id = item.Id }, new { @class = "btn btn-sm btn-primary" })

                        </td>
                    </tr>
                }

        </table>
    </div>
</div>
